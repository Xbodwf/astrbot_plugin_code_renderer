  "contains": [
    {
      "className": "comment",
      "variants": [
        {
          "begin": "//",
          "end": "$"
        },
        {
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    {
      "className": "string",
      "variants": [
        {
          "begin": "\"",
          "end": "\"",
          "illegal": "\\n",
          "contains": [
            {
              "begin": "\\\\.",
              "className": "string.escape"
            }
          ]
        }
      ]
    },
    {
      "className": "number",
      "variants": [
        {
          "begin": "\\b0[xX][0-9a-fA-F]+[Ll]?\\b"
        },
        {
          "begin": "\\b0[bB][01]+[Ll]?\\b"
        },
        {
          "begin": "\\b0[oO][0-7]+[Ll]?\\b"
        },
        {
          "begin": "\\b\\d*\\.\\d+([eE][\\-+]?\\d+)?[fFdD]?\\b"
        },
        {
          "begin": "\\b\\d+[Ll]?\\b"
        }
      ]
    },
    {
      "className": "meta",
      "begin": "#\\s*\\w+",
      "relevance": 10
    },
    {
      "className": "function",
      "beginKeywords": "function",
      "end": "\\{|;",
      "excludeEnd": true,
      "contains": [
        {
          "className": "title",
          "begin": "[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    {
      "className": "class",
      "beginKeywords": "class struct interface enum",
      "end": "\\{",
      "excludeEnd": true,
      "contains": [
        {
          "className": "title",
          "begin": "[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    {
      "className": "label",
      "begin": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*:",
      "relevance": 0
    }
  ]
}
